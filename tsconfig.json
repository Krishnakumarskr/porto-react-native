{
  "extends": [
    "expo/tsconfig.base"
  ],
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "exactOptionalPropertyTypes": false,
    "strict": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "allowImportingTsExtensions": true,
    "noPropertyAccessFromIndexSignature": true,
    "baseUrl": ".",
    "paths": {
      "#*": [
        "./*"
      ]
    }
  },
  "include": [
    "app/**/*",
    "./scripts/**/*",
    "_/App.tsx"
  ],
  "files": [
    "env.d.ts",
    "index.js",
    "reset.d.ts",
    "app.config.ts",
    "metro.config.js",
    "babel.config.js"
  ],
  "exclude": [
    "**/_/**",
    "node_modules",
    "**/node_modules/**"
  ]
}
